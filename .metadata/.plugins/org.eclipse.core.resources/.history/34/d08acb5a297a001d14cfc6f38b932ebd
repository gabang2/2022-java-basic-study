package ch16.game;

public class Player {
	PlayerLevel level;
	
	public Player() {
		this.level = new BeginnerLevel();
	}
	
	String getLevel() {
		return null;
	}
	
	void upgradeLevel(PlayerLevel playerLevel) {
		this.level = playerLevel;
	}
	
	final void play(int count) {
		level.go(count);
	}
	
}

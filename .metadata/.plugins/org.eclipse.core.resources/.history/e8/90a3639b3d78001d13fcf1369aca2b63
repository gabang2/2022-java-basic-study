package ch06;

import java.util.ArrayList;

public class CustomerTest {
	public static void main(String[] args) {
		ArrayList<Customer> customerList = new ArrayList<>();
		
		Customer customer1 = new Customer(10010, "가영");
		Customer customer2 = new Customer(10010, "가영");
		Customer customer3 = new Customer(10010, "가영");
		Customer customer4 = new Customer(10010, "가영");
		Customer customer5 = new Customer(10010, "가영");
		
		customerList.add(customer1);
		customerList.add(customer2);
		customerList.add(customer3);
		customerList.add(customer4);
		customerList.add(customer5);
		
		for(Customer customer : customerList) {
			System.out.println(customer.customerInfo());
		}
	}
}
